<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.CreateAccountController">
   <children>
      <AnchorPane layoutX="112.0" prefHeight="420.0" prefWidth="376.0" style="-fx-background-color: #9C2B27;">
         <children>
            <TextField fx:id="firstName" layoutX="53.0" layoutY="116.0" prefHeight="30.0" prefWidth="276.0" promptText="first name" style="-fx-background-color: #9C2B27; -fx-text-fill: #cfceee;">
               <font>
                  <Font name="Leelawadee UI" size="14.0" />
               </font>
               <effect>
                  <InnerShadow color="#cfceee" height="12.0" radius="7.75" />
               </effect>
            </TextField>
            <TextField fx:id="password" layoutX="53.0" layoutY="284.0" prefHeight="30.0" prefWidth="276.0" promptText="password" style="-fx-background-color: #9C2B27; -fx-text-fill: #cfceee;">
               <effect>
                  <InnerShadow color="#cfceee" />
               </effect>
               <font>
                  <Font size="14.0" />
               </font>
            </TextField>
            <TextField fx:id="email" layoutX="53.0" layoutY="229.0" prefHeight="30.0" prefWidth="276.0" promptText="email" style="-fx-background-color: #9C2B27; -fx-text-fill: #cfceee;">
               <effect>
                  <InnerShadow color="#cfceee" />
               </effect>
               <font>
                  <Font name="Leelawadee UI Semilight" size="14.0" />
               </font>
            </TextField>
            <TextField fx:id="lastName" layoutX="53.0" layoutY="170.0" prefHeight="30.0" prefWidth="276.0" promptText="last name" style="-fx-background-color: #9C2B27; -fx-text-fill: #cfceee;">
               <font>
                  <Font name="Leelawadee UI Semilight" size="14.0" />
               </font>
               <effect>
                  <InnerShadow color="#cfceee" />
               </effect>
            </TextField>
            <Label layoutX="112.0" layoutY="51.0" text="Create Account" textFill="#cfceee">
               <font>
                  <Font name="Leelawadee UI Semilight" size="24.0" />
               </font>
            </Label>
            <Button fx:id="nextButton" layoutX="154.0" layoutY="349.0" onAction="#handleNextButton" mnemonicParsing="false" style="-fx-background-color: #E45652;" text="Create" textFill="#cfceee">
               <font>
                  <Font name="Leelawadee UI Semilight" size="18.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>